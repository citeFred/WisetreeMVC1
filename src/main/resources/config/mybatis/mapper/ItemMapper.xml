<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.mapper.ItemMapper">
	<select id="selectByitemNum" resultType="Item" parameterType="_int">
		select itemNo,itemName,price,saleprice,point,quality,itemImage1,itemInfo
		from item2 where itemNo=#{value}
	</select>
	
</mapper>